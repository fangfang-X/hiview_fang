export GSF_CPU_ARCH=3531d
export GSF_DEV_TYPE=ipc
export GSF_HOME=`pwd`
export GSF_CC="aarch64-himix200-linux-gcc"
export GSF_AR=aarch64-himix200-linux-ar
export GSF_CFLAGS="-g -fstack-protector -fPIC -DGSF_CPU_3531d"
export GSF_LDFLAGS=""

if [ ! -d ${GSF_HOME}/lib/${GSF_CPU_ARCH} ]
then
	mkdir -p ${GSF_HOME}/lib/${GSF_CPU_ARCH}
fi

if [ ! -d ${GSF_HOME}/bin/${GSF_CPU_ARCH} ]
then
	mkdir -p ${GSF_HOME}/bin/${GSF_CPU_ARCH}
fi
